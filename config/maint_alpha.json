{
    "log"    : {
        "logLevel" : "info",
        "logDir"   : "/opt/sixxy/logs",
        "logName"  : "maint.log",
        "media"    : [ { "type" : "file" } ]
    },
    "caches" : {
        "run"     : "/opt/sixxy/run/",
        "line"    : "/opt/sixxy/share/dub/caches/line/",
        "script"  : "/opt/sixxy/share/dub/caches/script/",
        "video"   : "/opt/sixxy/share/dub/caches/video/",
        "output"  : "/opt/sixxy/share/dub/caches/output/"
    },
    "s3" : {
        "src" : {
            "bucket" : "c6.dev",
            "path"   : "media/src/screenjack/video"
            },
        "out" : {
            "bucket" : "c6.dev",
            "path"   : "media/usr/screenjack/video"
            },
        "share": {
            "bucket" : "c6.dev",
            "path"   : "media/usr/screenjack/data"
        }
    },
    "tts" : {
        "workspace" : "/tmp/"
    }
